t/property-list.mappers.js","original":{"line":52,"column":4}},{"generated":{"line":76,"column":9},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":11},"name":"queryParams"},{"generated":{"line":76,"column":20},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":22}},{"generated":{"line":76,"column":21},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":23},"name":"slice"},{"generated":{"line":76,"column":26},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":11}},{"generated":{"line":76,"column":27},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":29}},{"generated":{"line":76,"column":28},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":11}},{"generated":{"line":76,"column":30},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":32}},{"generated":{"line":76,"column":31},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":33}},{"generated":{"line":76,"column":32},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":11}},{"generated":{"line":76,"column":33},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":4}},{"generated":{"line":76,"column":34},"source":"pages/property-list/property-list.mappers.js","original":{"line":25,"column":48}},{"generated":{"line":76,"column":35},"source":"pages/property-list/property-list.mappers.js","original":{"line":52,"column":37}},{"generated":{"line":77,"column":0},"source":"pages/property-list/property-list.mappers.js","original":{"line":53,"column":1}},{"generated":{"line":77,"column":1},"source":"pages/property-list/property-list.mappers.js","original":{"line":25,"column":7}}],"sources":{"pages/property-list/property-list.mappers.js":"export const mapPropertyListfromApitoViewModel = propertyList => {\r\n    return propertyList.map(property => mapPropertyfromApitoViewModel(property));\r\n} // esto me para hacerlo para cada entidad en property List\r\n\r\n\r\nconst mapPropertyfromApitoViewModel = (property) => {  //esto es para una sola propiedad\r\n return {\r\n     id: property.id,\r\n     title: property.title,\r\n     rooms: `${property.rooms} ${getRoomWord(property.rooms)}`,\r\n     squareMeter: `${property.squareMeter}m2`,\r\n     notes: `${property.notes.substring(0, 240)}...`, //cogemos sólo desde el caracter 0 al 240\r\n     price: `${property.price.toLocaleString()}€`,\r\n     image: Array.isArray(property.images) ? property.images[0] : '', //si hay algo en property images coge la 1ª, si no vacío\r\n\r\n }\r\n}\r\n\r\nconst getRoomWord = (rooms) => {\r\n    return rooms > 1 ? \"habitaciones\" : \"habitación\";\r\n};\r\n\r\n/*mapear el filtro para construir las url (params)*/\r\n\r\nexport const mapFilterToQueryParams = filter => {\r\n    let queryParams = '';\r\n\r\n    if(filter.saleTypeId){\r\n        queryParams = `${queryParams}saleTypeIds_like=${filter.saleTypeId}&`; //_like= que el dato esté dentro de un array\r\n    };\r\n\r\n    if(filter.provinceId){\r\n        queryParams = `${queryParams}provinceId=${filter.provinceId}&`;\r\n    };\r\n\r\n    if(filter.minRooms){\r\n        queryParams = `${queryParams}rooms_gte=${filter.minRooms}&`;\r\n    };\r\n\r\n    if(filter.minBathrooms){\r\n        queryParams = `${queryParams}bathrooms_gte=${filter.minBathrooms}&`;\r\n    };\r\n\r\n    if(filter.minPrice){\r\n        queryParams = `${queryParams}price_gte=${filter.minPrince}&`;\r\n    };\r\n\r\n    if(filter.maxPrice){\r\n        queryParams = `${queryParams}price_lte=${filter.maxPrince}&`;\r\n    };   \r\n\r\n    return queryParams.slice(0, -1); //con el slice quitamos el último carácter (-1) \r\n}"},"lineCount":null}},"error":null,"hash":"6ef85393fd522bc7756199fb54b6a57e","cacheData":{"env":{}}}